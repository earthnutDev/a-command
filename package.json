{
  "type": "module",
  "private": true,
  "version": "2.1.7",
  "name": "a-command",
  "description": "一点点 🤏 终端工具 🧰",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "build": "jja rm dist cls&& npm run b && npm run clean:package",
    "clean:package": "node scripts/clean-package-json.js",
    "diff": "jja pkg --diff",
    "dev": "jja rm .earthnut cls cls  && rollup --config rollup.dev.config.js &&a_command=all a_node_tools=all node --unhandled-rejections=strict .earthnut/eg/index.mjs ",
    "push:version": "gvv",
    "push:submit": "gvv",
    "eslint": "eslint src",
    "beautify": "npm run prettier",
    "prettier": "prettier . --write",
    "test": "lint-staged && jest --coverage",
    "report": "cat ./coverage/lcov.info | coveralls",
    "coverage": "codecov",
    "vjj": "vjj -b",
    "prepublishOnly": "pjj",
    "prepare": "husky"
  },
  "license": "ISC",
  "lint-staged": {
    "**/*.{js,ts,tsx,jex,md,mdx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "codecov": "^3.8.3",
    "coveralls": "^3.1.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsdoc": "^50.6.11",
    "globals": "^16.0.0",
    "gvv": "^0.0.0",
    "husky": "^9.1.7",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jja": "^2.2.3",
    "lint-staged": "^15.5.1",
    "pjj": "^0.2.3",
    "prettier": "^3.5.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-obfuscator": "^1.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vjj": "^0.3.2"
  },
  "dependencies": {
    "@qqi/dev-log": ">=0.2.0 <1.0.0",
    "a-js-tools": ">=0.6.1 <1.0.0",
    "a-node-tools": ">=3.0.0 <4.0.0",
    "a-type-of-js": ">=0.2.0 <1.0.0",
    "color-pen": ">=1.3.1 <2.0.0"
  }
}

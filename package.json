{
  "type": "module",
  "private": true,
  "version": "2.0.0-next.1",
  "name": "a-command",
  "description": "终端的完整命令行代码（无法在浏览器环境中使用）",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "build": "jja rm dist cls&& npm run b && npm run clean:package",
    "clean:package": "node scripts/clean-package-json.js",
    "dev": "jja rm .earthnut cls cls  && rollup --config rollup.dev.config.js && node --unhandled-rejections=strict .earthnut/eg/index.mjs 56 awe  -a  a aa aaa -b bb 154 a bgh 212 c=10 k=200 bb 545 d 56 989 a 45 b bb=56 a=12 a=1",
    "push:version": "gvv",
    "push:submit": "gvv",
    "eslint": "eslint src",
    "beautify": "npm run prettier",
    "prettier": "prettier . --write",
    "test": "lint-staged && jest --coverage",
    "report": "cat ./coverage/lcov.info | coveralls",
    "coverage": "codecov",
    "vjj": "vjj",
    "prepublishOnly": "pjj",
    "prepare": "husky"
  },
  "license": "ISC",
  "lint-staged": {
    "**/*.{js,ts,tsx,jex,md,mdx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "globals": "^15.15.0",
    "gvv": "^0.0.0",
    "husky": "^9.1.7",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jja": "^1.0.3",
    "lint-staged": "^15.5.0",
    "pjj": "^0.1.4",
    "prettier": "^3.5.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-obfuscator": "^1.1.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vjj": "^0.1.2"
  },
  "dependencies": {
    "a-js-tools": ">=0.1.4 <1.0.0",
    "a-node-tools": ">=1.0.2 <2.0.0",
    "a-type-of-js": ">=0.2.0 <1.0.0",
    "color-pen": ">=0.1.3 <1.0.0"
  }
}

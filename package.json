{
  "type": "module",
  "private": true,
  "version": "0.1.0",
  "name": "a-command",
  "main": "cjs/index.cjs",
  "module": "mjs/index.mjs",
  "typings": "types/index.d.ts",
  "description": "终端的完整命令行代码（不允许在浏览器中使用）",
  "files": [
    "bin",
    "mjs",
    "cjs",
    "types"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./mjs/index.mjs",
        "types": "./types/index.d.ts"
      },
      "require": {
        "default": "./cjs/index.cjs",
        "types": "./types/index.d.ts"
      }
    },
    "./types/args": "./types/src/argTool/types.d.ts",
    "./types/question": "./types/src/question/types.d.ts",
    "./types/selection": "./types/src/selection/types.d.ts"
  },
  "scripts": {
    "build": "npm run b && npm run clean:package",
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "clean:package": "node scripts/clean-package-json.js",
    "push:version": "gvv",
    "push:submit": "gvv",
    "eslint": "eslint src",
    "beautify": "npm run prettier",
    "prettier": "prettier . --write",
    "test": "npx lint-staged && jest --coverage",
    "report": "cat ./coverage/lcov.info | coveralls",
    "coverage": "codecov",
    "vjj": "vjj",
    "prepublishOnly": "pjj",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthnutDev/a-command.git"
  },
  "author": {
    "name": "earthnut",
    "email": "earthnut.dev@outlook.com",
    "url": "https://earthnut.dev"
  },
  "license": "ISC",
  "keywords": [
    "a-command"
  ],
  "homepage": "https://earthnut.dev/a-command",
  "bugs": {
    "url": "https://github.com/earthnutDev/a-command/issues",
    "email": "earthnut.dev@outlook.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,jex,md,mdx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.28",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.5.0",
    "pjj": "^0.1.0",
    "prettier": "^3.5.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-obfuscator": "^1.1.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "bin": {
    "a-command": "bin/index.js"
  },
  "dependencies": {
    "a-js-tools": ">=0.1.3 <1.0.0",
    "a-node-tools": ">=0.1.1 <1.0.0",
    "a-type-of-js": "^0.1.6",
    "color-pen": ">=0.1.1 <1.0.0"
  }
}
